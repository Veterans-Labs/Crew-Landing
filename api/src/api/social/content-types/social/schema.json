{
  "kind": "collectionType",
  "collectionName": "socials",
  "info": {
    "singularName": "social",
    "pluralName": "socials",
    "displayName": "Social"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Name": {
      "type": "string"
    },
    "Url": {
      "type": "string",
      "default": "https://",
      "required": true,
      "regex": "^https?:\\/\\/\\w+(\\.\\w+)+(\\/[\\w\\.-]*)*\\/?$"
    },
    "ViewBox": {
      "type": "string",
      "default": "0 0 0 0",
      "required": true,
      "regex": "^\\d+\\s+\\d+\\s+\\d+\\s+\\d+$"
    },
    "Width": {
      "type": "integer"
    },
    "Height": {
      "type": "integer"
    },
    "Path": {
      "type": "text",
      "required": true
    }
  }
}
