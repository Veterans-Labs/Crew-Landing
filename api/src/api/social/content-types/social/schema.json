{
  "kind": "collectionType",
  "collectionName": "socials",
  "info": {
    "singularName": "social",
    "pluralName": "socials",
    "displayName": "Social"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "Url": {
      "type": "string",
      "regex": "^https?:\\/\\/[\\w\\-]+(\\.[\\w\\-]+)+[/#?]?.*$"
    },
    "ViewBox": {
      "type": "string",
      "required": true,
      "regex": "^\\d{1,8}(?:\\s\\d{1,8}){3}$"
    },
    "Width": {
      "type": "integer",
      "required": true
    },
    "Height": {
      "type": "integer",
      "required": true
    },
    "Path": {
      "type": "text",
      "required": true
    },
    "Order": {
      "type": "integer",
      "required": true,
      "unique": true
    }
  }
}
